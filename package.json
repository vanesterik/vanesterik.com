{
  "name": "vanesterik",
  "version": "1.0.0",
  "packageManager": "pnpm@8.2.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "devDependencies": {
    "@commitlint/config-conventional": "^17.7.0",
    "@vitest/coverage-v8": "^0.34.3",
    "commitlint": "^17.7.1",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.3",
    "prettier-config": "github:vanesterik/prettier-config",
    "typescript": "5.2.2",
    "vitest": "^0.34.3"
  },
  "scripts": {
    "build": "pnpm -r build",
    "coverage": "vitest --coverage",
    "lint": "pnpm -r lint",
    "prepare": "husky install",
    "test": "vitest",
    "typecheck": "pnpm -r typecheck"
  }
}
